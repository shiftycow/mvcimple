<?xml version="1.0"?>
<app title="Party Reservation">
    <models>
        <person>
            <id>
                <type>Integer</type>
                <primary_key>true</primary_key>
                <auto_increment>true</auto_increment>
            </id>
            <fname>
                <type>String</type>
                <length>10</length>
            </fname>

            <lname>
                <type>String</type>
                <length>10</length>
            </lname>

            <age>
                <type>Number</type>
            </age>
            <entry_date>
                <type>DateTime</type>
                <now></now>
            </entry_date>
        </person>
    </models>

</app>

